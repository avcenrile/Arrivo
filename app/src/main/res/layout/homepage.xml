<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/ejeepLineB"
        android:layout_width="310dp"
        android:layout_height="74dp"
        android:layout_marginTop="60dp"
        android:src="@drawable/ejeep_line_b"
        app:layout_constraintEnd_toEndOf="@+id/ejeepLineA"
        app:layout_constraintStart_toStartOf="@+id/ejeepLineA"
        app:layout_constraintTop_toBottomOf="@+id/ejeepLineA" />

    <ImageView
        android:id="@+id/arriveHPLogo"
        android:layout_width="175dp"
        android:layout_height="66dp"
        android:src="@drawable/arrivologov2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <ImageButton
        android:id="@+id/ejeepLineA"
        android:layout_width="310dp"
        android:layout_height="74dp"
        android:layout_marginTop="92dp"
        android:src="@drawable/ejeep_line_a"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arriveHPLogo" />

</androidx.constraintlayout.widget.ConstraintLayout>